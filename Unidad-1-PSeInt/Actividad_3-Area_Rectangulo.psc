Proceso Area_rectangulo
	Definir numero1, numero2, resultado Como Entero;
	
	Escribir "Ingrese la base de rectángulo";
	Leer numero1;
	
	Escribir "Ingrese la altura de rectángulo";
	Leer numero2;
	
	resultado=numero1*numero2;
	
	Escribir "El área del rectángulo es: .", resultado;
FinProceso